{
  "name": "form-create",
  "version": "1.5.4",
  "description": "具有动态渲染、数据收集、校验和提交功能的表单生成器，支持json、双向数据绑定、事件扩展以及自定义组件，可快速生成包含有省市区三级联动、时间选择、日期选择等17种功能组件。",
  "main": "dist/form-create.js",
  "module": "dist/form-create.js",
  "unpkg": "dist/form-create.js",
  "jsdelivr": "dist/form-create.js",
  "scripts": {
    "test": "echo \"Error:no test specified\" && exit 1",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --config=build/webpack.dist.dev.config.js",
    "build:prod": "webpack --config=build/webpack.dist.prod.config.js",
    "dev": "webpack-dev-server --content-base ./ --open --open-page demo/index.html --port 8081 --host 127.0.0.1 --config build/webpack.dev.config.js"
  },
  "files": [
    "README.md",
    "package.json",
    "LICENSE",
    "dist",
    "district",
    "demo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xaboy/form-create.git"
  },
  "keywords": [
    "form-create",
    "vue.js",
    "iview",
    "form",
    "form-generate",
    "form-builder",
    "components"
  ],
  "author": "xaboy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xaboy/form-create/issues"
  },
  "homepage": "http://www.form-create.com",
  "peerDependencies": {
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "compression-webpack-plugin": "^1.1.12",
    "html-webpack-plugin": "^3.2.0",
    "iview": "^3.1.4",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^2.11.3"
  },
  "dependencies": {},
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=5.5.1",
    "yarn": ">=1.3.2"
  },
